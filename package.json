    "dev": "yarn lingui:extract && yarn lingui:compile && next dev",
    "inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "yarn lingui:extract && yarn lingui:compile && next build",
    "start": "next start",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "circular": "madge --circular src --extensions ts,tsx,js,jsx",
    "unused": "next-unused",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{md,css}": "prettier --write"
